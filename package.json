{
  "name": "achievements-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "gen:openapi": "mkdir -p src/types && openapi-typescript openapi.json -o src/types/openapi.d.ts",
    "prebuild": "npm run gen:openapi",
    "postinstall": "npm run gen:openapi",
    "dev:types": "openapi-typescript --watch openapi.json -o src/types/openapi.d.ts"
  },
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^6.2.1",
    "algosdk": "^2.11.0",
    "next": "15.5.2",
    "rapidoc": "^9.3.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "ulujs": "^0.12.15"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "next-test-api-route-handler": "^5.0.2",
    "openapi-typescript": "^7.9.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
